<rolltemplate class="sheet-rolltemplate-attack">
    <table>
        <tr>
            <td class="sheet-detail" colspan="2"><i><b>{{character}}</b> attacks with {{weapon}} </i></td>    
        </tr>
        <tr>
            <td>Attack</td>
            <td><span class="tcat">{{attack}}</span></td>
        </tr>            
            {{#rollWasCrit() attack}}
        <tr>
                <td class="sheet-confirm">Confirm:</td>
                <td class="sheet-confirm"><span class="tcat">{{confirm}}</span></td>
        </tr>
            {{/rollWasCrit() attack}}
        <tr>
            <td>Damage</td>
            <td class="sheet-damage"><span class="tcat">{{damage}}</span></td>
        </tr>            
            {{#rollWasCrit() attack}}
        <tr>
                <td class="sheet-confirm">Crit:</td>
                <td class="sheet-confirm sheet-damage"><span class="tcat">{{dmgcrit}}</span></td>
        </tr>
            {{/rollWasCrit() attack}}
        <tr>
            <td colspan="2">{{details}}</td>
        </tr> 
        <tr>
            <td class="sheet-last_left"></td>
            <td class="sheet-last_right"></td>
        </tr> 
    </table>
</rolltemplate>
